{
    "version": 2,
    "builds": [
      {
        "src": "backend-express/index.js",   
        "use": "@vercel/node"
      }
    ],
    "hooks": {
      "build": "npx prisma generate"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend-express/index.js"   
      }
    ]
  }
  